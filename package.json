{
  "name": "express-2020",
  "version": "0.2.5",
  "description": "The only express boilerplate you need for build better servers",
  "main": "dist/app.js",
  "engines": {
    "node": "14.13.x"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "inspect": "cross-env NODE_ENV=development  nodemon --inspect",
    "start": "cross-env NODE_ENV=production  pm2 start dist/app.js -i max",
    "build": "cross-env NODE_ENV=production  babel src -d dist --ignore=\"./src/test-runners\" --extensions \".ts\""
  },
  "keywords": [
    "express",
    "boilerplate",
    "generator",
    "express-typescript",
    "esnext",
    "node",
    "node-express"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MagicRomero/node-express-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/MagicRomero/node-express-boilerplate.git/issues"
  },
  "author": "Magic romero",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/app-root-path": "^1.2.4",
    "@types/argon2": "^0.15.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/express-jwt": "0.0.42",
    "@types/express-mongo-sanitize": "^1.3.2",
    "@types/express-rate-limit": "^5.1.0",
    "@types/express-winston": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.11.2",
    "@types/winston": "^2.4.4",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "argon2": "^0.27.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
